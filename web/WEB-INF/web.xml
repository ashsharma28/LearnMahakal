<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
		 
		 
		 
		 
		 
    <filter>
        <filter-name>ViewAndAnalysis</filter-name>
        <filter-class>com.vibhuti.lms.filters.ViewAndAnalysisFilter</filter-class>
    </filter>


    <filter-mapping>
        <filter-name>ViewAndAnalysis</filter-name>
        <url-pattern>/*</url-pattern>

    </filter-mapping>


    <context-param>
        <param-name>file-upload</param-name>
        <param-value>F:\Dev\LearningManagementSystem\src\main\webapp\data\</param-value>
        <description>Location to store uploaded file</description>
    </context-param>


    <context-param>
        <param-name>attendance-upload</param-name>
        <param-value>F:\Dev\LearningManagementSystem\src\main\webapp\attendance\</param-value>
        <description>Location to store uploaded attendance</description>
    </context-param>



    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
	
	
	
	
	
</web-app>